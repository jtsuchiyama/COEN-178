SQL> Create or Replace Procedure setEmpSalary(p_name in VARCHAR, low in
  2  INTEGER, high in INTEGER)
  3  As
  4  Cursor Emp_cur IS
  5  	     SELECT * FROM AlphaCoEmp;
  6  	     l_emprec Emp_cur%rowtype;
  7  	     l_salary AlphaCoEmp.salary%type;
  8  BEGIN
  9  	     for l_emprec IN Emp_cur
 10  	     loop
 11  		     l_salary := ROUND(dbms_random.value(low,high));
 12  		     UPDATE AlphaCoEmp
 13  	     SET salary = l_salary
 14  	     WHERE name = p_name;
 15  END LOOP;
 16  commit;
 17  END;
