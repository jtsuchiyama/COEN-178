SQL> Create or Replace Procedure setSalaries(low in INTEGER, high in INTEGER)
  2  As
  3  Cursor Emp_cur IS
  4  	     Select * from AlphaCoEmp;
  5  	     -- Local variables
  6  	     l_emprec Emp_cur%rowtype;
  7  	     l_salary AlphaCoEmp.salary%type;
  8  BEGIN
  9  	     for l_emprec IN Emp_cur
 10  	     loop
 11  		     l_salary := ROUND(dbms_random.value(low,high));
 12  		     update AlphaCoEmp
 13  	     set salary = l_salary
 14  	     where name = l_emprec.name;
 15  END LOOP;
 16  commit;
 17  END;
