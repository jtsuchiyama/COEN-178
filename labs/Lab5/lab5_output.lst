SQL> Create or Replace Function calcSalaryRaise( p_name in
  2  AlphaCoEmp.name%type, percentRaise IN NUMBER)
  3  RETURN NUMBER
  4  	     IS
  5  		     l_salary AlphaCoEmp.salary%type;
  6  		     l_raise AlphaCoEmp.salary%type;
  7  		     l_cnt Integer;
  8  BEGIN
  9  	     -- Find the current salary of p_name from AlphaCoEMP table.
 10  	     Select salary into l_salary from AlphaCoEmp
 11  	     where name = p_name;
 12  	     -- Calculate the raise amount
 13  	     l_raise := l_salary * (percentRaise/100);
 14  	     -- Check if the p_name is in Emp_Work table.
 15  	     -- If so, add a $1000 bonus to the
 16  	     -- raise amount
 17  	     Select count(*) into l_cnt from Emp_Work
 18  	     where name = p_name;
 19  	     if l_cnt >= 1 THEN
 20  	     l_raise := l_raise + 1000;
 21  End IF;
 22  	     /* return a value from the function */
 23  	     return l_raise;
 24  END;
 25  /

