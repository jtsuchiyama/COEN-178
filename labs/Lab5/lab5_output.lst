SQL> Create or Replace Procedure assignJobTitlesAndSalaries
  2  As
  3  	     type titlesList IS VARRAY(6) OF AlphaCoEmp.title%type;
  4  	     type salaryList IS VARRAY(6) of AlphaCoEmp.salary%type;
  5  	     v_titles titlesList;
  6  	     v_salaries salaryList;
  7  Cursor Emp_cur IS
  8  	     Select * from AlphaCoEmp;
  9  	     l_emprec Emp_cur%rowtype;
 10  	     l_title AlphaCoEmp.title%type;
 11  	     l_salary AlphaCoEmp.salary%type;
 12  	     l_randomnumber INTEGER := 1;
 13  
 14  BEGIN
 15  /* Titles are stored in the v_titles array */
 16  /* Salaries for each title are stored in the v_salaries array.
 17  The salary of v_titles[0] title is at v_salaries[0].
 18  */
 19  v_titles := titlesList('advisor', 'director', 'assistant', 'manager','supervisor','bigboss');
 20  v_salaries := salaryList(130000, 100000, 600000, 500000, 800000,'4200000');
 21  /* use a for loop to iterate through the set */
 22  for l_emprec IN Emp_cur
 23  LOOP
 24  	     /* We get a random number between 1-5 both inclusive */
 25  	     l_randomnumber := dbms_random.value(1,6);
 26  	     /* Get the title using the random value as the index into the v_tiles array */
 27  	     l_title := v_titles(l_randomnumber);
 28  	     /* Get the salary using the same random value as the index into the v_salaries array */
 29  	     l_salary := v_salaries(l_randomnumber);
 30  	     /* Update the employee title and salary using the l_title and l_salary */
 31  	     update AlphaCoEmp
 32  	     set title = l_title
 33  	     where name = l_emprec.name;
 34  
 35  	     update AlphaCoEmp
 36  	     set salary = l_salary
 37  	     where name = l_emprec.name;
 38  END LOOP;
 39  
 40  commit;
 41  END;
 42  /

